{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "ConnectionString": "mongodb://localhost:27017,localhost:27018,localhost:27019/?replicaSet=rs0&serverSelectionTimeoutMS=60000&connectTimeoutMS=60000&socketTimeoutMS=90000&maxPoolSize=200&waitQueueTimeoutMS=10000&retryWrites=true&retryReads=true",
    "DatabaseName": "TodoDb"
  },
  "OutboxSettings": {
    "ProcessingIntervalSeconds": 10,
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 60,
    "BatchSize": 10,
    "AutoStartProcessor": true,
    "ProcessingTtlMinutes": 5
  }
}